ninja_required_version = 1.3
cxx = c++
nvcc = /mnt/lustre/share/cuda-10.1/bin/nvcc

cflags = -pthread -B /mnt/lustre/lpan/anaconda3/envs/mvp/compiler_compat -Wl,--sysroot=/ -Wsign-compare -DNDEBUG -g -fwrapv -O3 -Wall -Wstrict-prototypes -fPIC -I/mnt/lustre/lpan/anaconda3/envs/mvp/lib/python3.7/site-packages/torch/include -I/mnt/lustre/lpan/anaconda3/envs/mvp/lib/python3.7/site-packages/torch/include/torch/csrc/api/include -I/mnt/lustre/lpan/anaconda3/envs/mvp/lib/python3.7/site-packages/torch/include/TH -I/mnt/lustre/lpan/anaconda3/envs/mvp/lib/python3.7/site-packages/torch/include/THC -I/mnt/lustre/share/cuda-10.1/include -I/mnt/lustre/lpan/anaconda3/envs/mvp/include/python3.7m -c
post_cflags = -DTORCH_API_INCLUDE_EXTENSION_H -DTORCH_EXTENSION_NAME=emd -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++14
cuda_cflags = -I/mnt/lustre/lpan/anaconda3/envs/mvp/lib/python3.7/site-packages/torch/include -I/mnt/lustre/lpan/anaconda3/envs/mvp/lib/python3.7/site-packages/torch/include/torch/csrc/api/include -I/mnt/lustre/lpan/anaconda3/envs/mvp/lib/python3.7/site-packages/torch/include/TH -I/mnt/lustre/lpan/anaconda3/envs/mvp/lib/python3.7/site-packages/torch/include/THC -I/mnt/lustre/share/cuda-10.1/include -I/mnt/lustre/lpan/anaconda3/envs/mvp/include/python3.7m -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options '-fPIC' -DTORCH_API_INCLUDE_EXTENSION_H -DTORCH_EXTENSION_NAME=emd -D_GLIBCXX_USE_CXX11_ABI=0 -gencode=arch=compute_70,code=sm_70 -std=c++14
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  command = $nvcc $cuda_cflags -c $in -o $out $cuda_post_cflags



build /mnt/lustre/lpan/workspaces/MVP_Benchmark/completion/build/temp.linux-x86_64-3.7/emd.o: compile /mnt/lustre/lpan/workspaces/MVP_Benchmark/completion/emd.cpp
